<job>
    <executable>bmp_est</executable>
    <directory>${GLOBUS_USER_HOME}</directory>
    <argument>1.bmp</argument>
    <argument>2.bmp</argument>
    <stdout>${GLOBUS_USER_HOME}/stdout</stdout>
    <stderr>${GLOBUS_USER_HOME}/stderr</stderr>
    <fileStageIn>
        <transfer>
            <sourceUrl>gsiftp://cormoran.arcos.inf.uc3m.es:2811/tmp/bmp_est</sourceUrl>
            <destinationUrl>file:///${GLOBUS_USER_HOME}/bmp_est</destinationUrl>
        </transfer>
        <transfer>
            <sourceUrl>gsiftp://cormoran.arcos.inf.uc3m.es:2811/tmp/1.bmp</sourceUrl>
            <destinationUrl>file:///${GLOBUS_USER_HOME}/1.bmp</destinationUrl>
        </transfer>
    </fileStageIn>
    <fileStageOut>
        <transfer>
            <sourceUrl>file:///${GLOBUS_USER_HOME}/2.bmp</sourceUrl>
            <destinationUrl>gsiftp://cormoran.arcos.inf.uc3m.es:2811/tmp/2.bmp</destinationUrl>
        </transfer>
    </fileStageOut>
    <fileCleanUp>
        <deletion>
            <file>file:///${GLOBUS_USER_HOME}/bmp_est</file>
        </deletion>
    </fileCleanUp>
</job>
